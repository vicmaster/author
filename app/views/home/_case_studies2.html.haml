.container
  .empty-space.marg-lg-b95.marg-sm-b50
  .row
    .col-sm-10.col-sm-offset-1
      / TT-TITLE
      .tt-title
        .tt-title-cat Consigue la atraccion que quieres
        %h2.c-h2
          %small Casos de Estudio
      .empty-space.marg-lg-b40.marg-sm-b30
      / TT-TAB-WRAPPER TYPE-1
      .tt-tab-wrapper.type-1
        .tt-tab-nav-wrapper
          .tt-tab-select
            .select-arrow
              %i.fa.fa-angle-down
            %select
              %option{ :selected => "selected" } Caso 1
              %option Caso 2
              %option Caso 3
              %option Caso 4
          .tt-nav-tab.mbottom50
            .tt-nav-tab-item.active
              %span.lnr.lnr-rocket
              %span.tt-analitics-text Caso 1
            .tt-nav-tab-item
              %span.lnr.lnr-smile
              %span.tt-analitics-text Caso 2
            .tt-nav-tab-item
              %span.lnr.lnr-thumbs-up
              %span.tt-analitics-text Caso 3
            .tt-nav-tab-item
              %span.lnr.lnr-user
              %span.tt-analitics-text Caso 4
        .tt-tabs-content.clearfix.mbottom50
